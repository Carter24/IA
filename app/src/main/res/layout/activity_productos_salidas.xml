<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ProductosSalidas">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text=""
            android:ems="10"
            android:id="@+id/FechaSalida" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="232dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.502"/>
    <Button
            android:text="Ver"
            android:layout_width="96dp"
            android:layout_height="51dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/VerSalida"
            android:layout_marginBottom="224dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="72dp" app:layout_constraintStart_toEndOf="@+id/AgregarEntrada"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.0"
    />
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:inputType="textPersonName"
            android:text=""
            android:ems="10"
            android:id="@+id/NombreProductoSalida"
            android:layout_marginTop="188dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.502"
            android:textStyle="normal"/>
    <Button
            android:text="Salida"
            android:layout_width="90dp"
            android:layout_height="52dp"
            android:id="@+id/AgregarSalida"
            app:layout_constraintEnd_toStartOf="@+id/VerSalida"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="72dp" app:layout_constraintHorizontal_bias="0.086"
            android:layout_marginBottom="224dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="8dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text=""
            android:ems="10"
            android:id="@+id/CantidadSalida" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="284dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.502"/>
</android.support.constraint.ConstraintLayout>