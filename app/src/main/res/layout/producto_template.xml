<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="431dp"
            android:layout_height="3dp" tools:layout_editor_absoluteX="-7dp"
            android:background="@color/colorPrimaryDark" app:layout_constraintTop_toBottomOf="@+id/CantidadEntrada2"
            android:layout_marginTop="20dp">
    </LinearLayout>
    <TextView
            android:layout_width="237dp"
            android:layout_height="45dp"
            android:id="@+id/CantidadEntrada2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.167"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/FechaEntrada2"
            android:textStyle="bold" android:textSize="18sp"/>
    <TextView
            android:layout_width="237dp"
            android:layout_height="39dp"
            android:id="@+id/FechaEntrada2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.167"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/NombreProductoEntrada2"
            android:textStyle="bold" android:textSize="18sp"/>
    <TextView
            android:layout_width="237dp"
            android:layout_height="39dp"
            android:id="@+id/IdInventarioEntrada" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.167"
            android:layout_marginTop="16dp" app:layout_constraintTop_toTopOf="parent" android:textStyle="bold"
            android:textSize="18sp"/>
    <TextView
            android:layout_width="237dp"
            android:layout_height="39dp"
            android:id="@+id/NombreProductoEntrada2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.167"
            app:layout_constraintTop_toBottomOf="@+id/IdInventarioEntrada" android:layout_marginTop="8dp"
            android:textStyle="bold" android:textSize="18sp"/>
</android.support.constraint.ConstraintLayout>